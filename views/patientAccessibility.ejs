    <h1 class="header">Time it Takes a Patient to Get an Appointment</h1>
    <table class="data_table" id="patient_table" >
        <tr>
            <th>AmountOfTime</th>
            <th>PercentAmount</th>
            <th>TotalAmount</th>
            <th>SpecifcAmount</th>
        </tr>
        <% patient.forEach(function(o) { %>
            <tr><td><%= o.AmountOfTime %></td><td><%= o.PercentAmount %></td><td><%= o.TotalAmount %></td><td><%= o.SpecifcAmount %></td></tr>
        <% }); %>
    </table>
    <div id="canvas_container">
        <canvas id="myChart" width="200" height="200"></canvas>
    </div>
    <script src="javascript/array.js" type="text/javascript"></script>
    <script>
        const chosenField = "PercentAmount"
        let dataArray = returnArrayFromTable("patient_table");
        let valuesArray = desiredArrayByYear("All Years", dataArray, chosenField);
        let labelArray = makeLabelArray(dataArray, "AmountOfTime", false);
        let colorArray = ['rgb(66, 179, 229 )', 'rgb(45, 187, 204 )', 'rgb(23, 195, 178 )', 'rgb(13, 164, 192 )', 'rgb(3, 132, 206 )', 'rgb(26, 109, 185 )', 'rgb(49, 86, 163 )', 'rgb(11, 49, 66 )'];
        let globalChart = generateChartNoLabelsAll(labelArray, valuesArray, colorArray, "How Accessible", 'pie' )

