<!DOCTYPE html>
<html>
    <head>
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css" integrity="sha384-HSMxcRTRxnN+Bdg0JdbxYKrThecOKuH5zCYotlSAcp1+c8xmyTe9GYg1l9a69psu" crossorigin="anonymous">
        <link type="text/css" href="css/partialGraph.css" rel="stylesheet">
    </head>
    <body>
        <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
        <h2 class="header">Volunteers</h2>
        <table class="data_table" id="patient_tableVOL" >
            <tr>
				<th>Type</th>
                <th>Amount of Volunteers</th>
                <th>Year</th>
            </tr>
			<% volunteers.forEach(function(o) { %>
            <tr><td><%= o.VolunteerType %></td> <td><%= o.VolunteerAmount %></td><td><%= o.RecordedYear %></td></tr>
			<% }); %>
        </table>
        <div id="canvas_container">
            <canvas id="myChartVOL" ></canvas>
        </div>
        <script src="javascript/array.js" type="text/javascript"></script>
        <script>
        const chosenFieldVOL = "VolunteerAmount"
        let dataArrayVOL = returnArrayFromTable("patient_tableVOL");
        let yearsVOL = makeYearArray(dataArrayVOL, "RecordedYear");
        let valuesArrayVOL = desiredArrayByYear("All Years", dataArrayVOL, chosenFieldVOL);
        let labelArrayVOL = makeLabelArray(dataArrayVOL, "VolunteerType", true, false);
        let colorArrayVOL = makeMultipleColorArray(2, false, yearsVOL.length );
        let globalChartVOL = generateChartPartial(labelArrayVOL, valuesArrayVOL, colorArrayVOL, "Number Of Patient Applications", 'bar', "myChartVOL" );

        </script>
           
    </body>
</html>

 