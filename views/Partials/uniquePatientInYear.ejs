<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css" integrity="sha384-HSMxcRTRxnN+Bdg0JdbxYKrThecOKuH5zCYotlSAcp1+c8xmyTe9GYg1l9a69psu" crossorigin="anonymous">
    <link type="text/css" href="css/partialGraph.css" rel="stylesheet">
    <title>Patient Medical Home</title>
</head>
<body>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
        <h1 class="header">Unique Patients in a year</h1>
        <table class="data_table" id="patient_tableUP" >
            <tr>
                <th>AmountOfYears</th>
                <th>PatientAmount</th>
            </tr>
            <% patient.forEach(function(o) { %>
                <tr><td><%= o.AmountOfYears %></td><td><%= o.PatientAmount %></td></tr>
            <% }); %>
        </table>
        <div class="canvas_container">
            <canvas id="myChartUP" ></canvas>
        </div>
        <script src="javascript/array.js" type="text/javascript"></script>
    <script>
        //name of column you want data from
        const chosenFieldUP = "PatientAmount"
        //giving function table id
        let dataArrayUP = returnArrayFromTable("patient_tableUP");
        //gets array of values
        let valuesArrayUP = desiredArrayByYear("All Years", dataArrayUP, chosenFieldUP);
        //gets labels
        let labelArrayUP = makeLabelArray(dataArrayUP, "AmountOfYears", false);
        //gets colors array - currently can only create array of one color
        let colorArrayUP = makeColorArray(dataArrayUP);
        //creates graph
        let globalChartUP = generateChartPartial(labelArrayUP, valuesArrayUP, colorArrayUP, "PatientAmount", 'bar', "myChartUP")
        //makes dropdown
        

    </script>
</body>
</html>