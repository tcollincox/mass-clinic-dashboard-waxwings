<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css" integrity="sha384-HSMxcRTRxnN+Bdg0JdbxYKrThecOKuH5zCYotlSAcp1+c8xmyTe9GYg1l9a69psu" crossorigin="anonymous">
    <link type="text/css" href="css/partialGraph.css" rel="stylesheet">
    <title>Diabetic Blood Pressure Testing 2019</title>
</head>
<body>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
        <div class="content">
        <h2 class="header">Diabetic Blood Pressure Testing 2019</h2>
        <table class="data_table" id="patients_tableDBPT" >
            <tr>
                <th>PatientAmount</th>
                <th>LevelOfControl</th>
            </tr>
            <% patients.forEach(function(o) { %>
                <tr><td><%= o.PatientAmount %></td><td><%= o.LevelOfControl %></td></tr>
            <% }); %>
        </table>
        <div class="canvas_container">
            <canvas id="myChartDBPT"></canvas>
        </div>

        <script src="javascript/array.js" type="text/javascript"></script>
    <script>
        //name of column you want data from
        const chosenFieldDBPT = "PatientAmount"
        //giving function table id
        let dataArrayDBPT = returnArrayFromTable("patients_tableDBPT");
        //gets array of values
        let valuesArrayDBPT = desiredArrayByYear("All Years", dataArrayDBPT, chosenFieldDBPT);
        //gets labels
        let labelArrayDBPT = makeLabelArray(dataArrayDBPT, "LevelOfControl", false);
        //gets colors array - currently can only create array of one color
        let colorArrayDBPT = makeColorArray(dataArrayDBPT);
        //creates graph
        let globalChartDBPT = generateChartPartial(labelArrayDBPT, valuesArrayDBPT, colorArrayDBPT, "PatientAmount", 'bar', "myChartDBPT" );

    </script>
     </div>
</body>
</html>