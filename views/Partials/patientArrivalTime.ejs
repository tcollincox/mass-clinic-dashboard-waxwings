<!DOCTYPE html>
<html>
    <head>
        <title>
            Patient Arrival Times
        </title>
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css" integrity="sha384-HSMxcRTRxnN+Bdg0JdbxYKrThecOKuH5zCYotlSAcp1+c8xmyTe9GYg1l9a69psu" crossorigin="anonymous">
        <link type="text/css" href="css/partialGraph.css" rel="stylesheet">
        </style>
    </head>
    <body>
        <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
        <h1 class="header">Arrival Times of MASS Clinic Patients</h1>
        <table class="data_table" id="patient_tablePAT" >
            <tr>
                <th>TimeArrived</th>
                <th>PercentAmount</th>
                <th>PatientAmount</th>
            </tr>
            <% patientArrivalTimes.forEach(function(o) { %>
                <tr><td><%= o.TimeArrived %></td><td><%= o.PercentAmount %></td><td><%= o.PatientAmount %></td></tr>
            <% }); %>
        </table>
        <div id="canvas_container">
            <canvas id="myChartPAT" width="200" height="200"></canvas>
        </div>
        
        <script src="javascript/array.js" type="text/javascript"></script>
        <script>
            let chosenFieldPTA = "PercentAmount"
            let dataArrayPTA = returnArrayFromTable("patient_tablePAT");
            let valuesArrayPTA = desiredArrayByYear("All Years", dataArrayPTA, chosenFieldPTA);
            let labelArrayPTA = makeLabelArray(dataArrayPTA, "TimeArrived", false);
            let colorArrayPTA = makeMultipleColorArray(dataArrayPTA.length, false, 1);
            let globalChartPTA = generateChartPartial(labelArrayPTA, valuesArrayPTA, colorArrayPTA, "How Accessible", 'pie', 'myChartPAT');
        </script>
    </body>
</html>