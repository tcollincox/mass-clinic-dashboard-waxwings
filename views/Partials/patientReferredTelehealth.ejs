<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css" integrity="sha384-HSMxcRTRxnN+Bdg0JdbxYKrThecOKuH5zCYotlSAcp1+c8xmyTe9GYg1l9a69psu" crossorigin="anonymous">
    <link type="text/css" href="css/partialGraph.css" rel="stylesheet">
    <title>Patient Referred To Telehealth</title>
</head>
<body>
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
        <div class="content">
        <h2 class="header">Patients Referred to Telehealth</h2>
        <table class="data_table" id="patient_tableprt" >
            <tr>
                <th>PatientType</th>
                <th>PatientAmount</th>
                <th>RecordedYear</th>
            </tr>
            <% patient.forEach(function(o) { %>
                <tr><td><%= o.PatientType %></td><td><%= o.PatientAmount %></td><td><%= o.RecordedYear %></td></tr>
            <% }); %>
        </table>
        <div class="canvas_container">
            <canvas id="myChartprt"></canvas>
        </div>
        <script src="javascript/array.js" type="text/javascript"></script>
    <script>
        //name of column you want data from
        const chosenFieldprt = "PatientAmount"
        //giving function table id
        let dataArrayprt = returnArrayFromTable("patient_tableprt");
        //gets array of values
        let valuesArrayprt = desiredArrayByYear("All Years", dataArrayprt, chosenFieldprt);
        //gets labels
        let labelArrayprt = makeLabelArray(dataArrayprt, "PatientType", false);
        //gets colors array - currently can only create array of one color
        let colorArrayprt = makeColorArray(dataArrayprt);
        //creates graph
        let globalChartprt = generateChartPartial(labelArrayprt, valuesArrayprt, colorArrayprt, "PatientAmount", 'bar','myChartprt' )
        //makes dropdown
    </script>

</body>
</html>