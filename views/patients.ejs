<!DOCTYPE html>
<html>
    <head>
        <title>
            Patient Screenings 
        </title>
    </head>
    <body>
        <h1>Screenings</h1>
        <p><a href="/"><button>Home</button></a></p> 
        <% patient.forEach(function(p) { %>
            <li><span>Id:  <%= p.userId %>, Number of Screenings:  <%= p.screenings %>, Year:  <%= p.year %></span></li>
        <% }); %>
    </body>
</html>